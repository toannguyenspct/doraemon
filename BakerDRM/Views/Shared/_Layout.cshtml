<body>
    @*<script src="@RouteJs.RouteJsHandler.HandlerUrl"></script>`
        <script src="@EnumHandler.HandlerUrl"></script>
        @Scripts.Render("~/Bundles/js")
        @RenderSection("scripts", required: false)*@
    @*@section scripts{}*@
    <!--================Main Header Area =================-->
    @Html.Partial("header")
    <!--================End Main Header Area =================-->
    <!--================Slider Area =================-->
    @RenderBody()
    <!--================End Newsletter Area =================-->

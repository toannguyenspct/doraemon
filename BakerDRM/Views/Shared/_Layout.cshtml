    <script src="~/dist/vendors/magnifc-popup/jquery.magnific-popup.min.js"></script>
    <script src="~/dist/vendors/datetime-picker/js/moment.min.js"></script>
    <script src="~/dist/vendors/datetime-picker/js/bootstrap-datetimepicker.min.js"></script>
    <script src="~/dist/vendors/nice-select/js/jquery.nice-select.min.js"></script>
    <script src="~/dist/vendors/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/dist/vendors/lightbox/simpleLightbox.min.js"></script>

    <script src="~/dist/js/theme.js"></script>
